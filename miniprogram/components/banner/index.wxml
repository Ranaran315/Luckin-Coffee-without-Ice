<!--components/banner/index.wxml-->
<view class="banner">
	<swiper autoplay="true" indicator-dots="true" indicator-color="#c1c1c1" indicator-active-color="#fff">
	<block wx:for="{{bannerList}}" wx:key="id">
		<swiper-item>
			<image src="{{item.src}}" mode="aspectFill" />
		</swiper-item>
	</block>
</swiper>
</view>